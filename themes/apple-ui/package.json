{
  "name": "@asm/apple-ui",
  "version": "0.0.19",
  "description": "Assemble Component Library",
  "author": "Assemble Frontend Team",
  "license": "MIT",
  "main": "./dist/style.scss",
  "module": "./dist/style.scss",
  "types": "./dist/style.scss.d.ts",
  "files": [
    "/dist/*"
  ],
  "enginesStrict": true,
  "engines": {
    "node": "^12.16.0 || ^14.17.0 || >=16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.3"
  },
  "scripts": {
    "compile": "sass src/*.scss dist/style.css",
    "build": "rm -rf ./dist && mkdir dist && yarn compile && yarn typed-scss-modules src/*.scss && cp src/* dist/",
    "prepublish": "rm -rf ./dist && yarn build"
  },
  "devDependencies": {
    "@asm/tsconfig": "*",
    "sass": "^1.55.0",
    "typed-scss-modules": "^7.0.1"
  },
  "dependencies": {}
}
