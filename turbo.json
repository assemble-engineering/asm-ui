{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "./dist"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": [
        "^lint"
      ]
    },
    "lint:fix": {
      "outputs": [],
      "dependsOn": [
        "^lint:fix"
      ]
    },
    "deploy": {
      "dependsOn": [
        "build",
        "test",
        "lint"
      ],
      "outputs": []
    }
  }
}
